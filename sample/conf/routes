# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST        /first/:param1/second/:param2/upload                  controllers.Application.upload(param1: String, param2: String)
GET         /file/:filename                                       controllers.Application.serveFile(filename: String)

POST        /scan                                                 controllers.Application.scanFile
POST        /scanTemp                                             controllers.Application.scanTempFile

POST        /direct                                               controllers.Application.directUpload